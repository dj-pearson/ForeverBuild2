        self:PurchaseCoins(product.id)
    end)
    
    return option
end

-- Purchase coins
function CurrencyUI:PurchaseCoins(productId)
    MarketplaceService:PromptProductPurchase(self.player, productId)
end

return CurrencyUI ]]></string>
